        android:padding="7dp"
        android:textColor="#213646"
        android:textColorHint="#213646"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Years_of_seniority"
        app:layout_constraintVertical_bias="0.322"
