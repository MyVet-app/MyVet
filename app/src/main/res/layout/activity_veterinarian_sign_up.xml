        android:textColor="#E20B2A"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/location_input"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.951"
